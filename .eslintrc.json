{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "no-console": 0,
        "new-cap":[0],
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
         ]
    }
}
